begin gnome-session-save
	begin
	 prog = irxevent
	 remote = RM-820
	 button = up
	 repeat = 1
	 config = Key Up CurrentWindow
	end

	begin
	 prog = irxevent
	 remote = RM-820
	 button = down
	 repeat = 1
	 config = Key Down CurrentWindow
	end

	begin
	 prog = irxevent
	 remote = RM-820
	 button = ok
	 repeat = 1
	 config = Key Return CurrentWindow
	end
end gnome-session-save
